{
    "serverName": "MyHttpServer",
    "serverVersion": "1.1",
    "address": "127.0.0.1",
    "port": 8080,
    "poolSize": 10,
    "networkReadBuffer": 8096,
    "modulesPath": "modules",
    "modules": {
        "zia-log": 1,
        "zia-secure_network": 2,
        "zia-unsecure_network": 3
    },
    "modulesProperties": {
        "tls_certificate_path": "tls/cert.crt",
        "tls_private_key_path": "tls/private.key"
    }
}